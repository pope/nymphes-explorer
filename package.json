{
  "name": "nymphes-explorer",
  "version": "0.0.1",
  "description": "A web app for poking around the Dreadbox Nymphes",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "build": "parcel build",
    "check": "tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write --ignore-unknown .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "parcel"
  },
  "devDependencies": {
    "@types/webmidi": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "import-sort-style-module": "^6.0.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "rxjs": "^7.5.6"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
